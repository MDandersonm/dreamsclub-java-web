<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace ="xyz.dreams.mapper.ReviewMapper">

	<!-- 강민경: 댓글 등록 -->
	<insert id="enrollReply">
		insert into Review(
			REV_CONT,
			GOODS_STAR,
			MEMBER_ID
			REV_DATE
		)values(
			#{revCont},
			#{goodsStar},			
			#{memberId},
			SYSDATE
		)
		
	</insert>
	
</mapper>
